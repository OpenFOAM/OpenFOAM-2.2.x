FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 100000;

boundaryField
{
    walls
    {
        type            buoyantPressure;
        gradient        uniform 0;
        value           uniform 100000;
    }
    inlet
    {
        type            buoyantPressure;
        gradient        uniform 0;
        value           uniform 100000;
    }
    outlet
    {
        type            fixedValue;
        value           uniform 100000;
    }
    blades
    {
        type            buoyantPressure;
        value           uniform 100000;
    }
}


