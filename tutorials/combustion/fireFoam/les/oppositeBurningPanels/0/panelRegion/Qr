FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      Qr;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 0 -3 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    ".*"
    {
        type            zeroGradient;
    }

    fRight_zone_side
    {
        type            wedge;
    }

    fLeft_zone_side
    {
        type            wedge;
    }

    region0_to_panelRegion_left_face
    {
        type            mappedField;
        sampleRegion    region0;
        sampleMode      nearestPatchFace;
        samplePatch     region0_to_panelRegion_left_face;
        offset          (0 0 0);
        fieldName       Qr;
        setAverage      no;
        average         0;
        value           uniform 0;
    }

    region0_to_panelRegion_right_face
    {
        type            mappedField;
        sampleRegion    region0;
        sampleMode      nearestPatchFace;
        samplePatch     region0_to_panelRegion_right_face;
        offset          (0 0 0);
        fieldName       Qr;
        setAverage      no;
        average         0;
        value           uniform 0;
    }
}


