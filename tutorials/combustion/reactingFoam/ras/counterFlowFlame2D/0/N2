FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      O2;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.77;

boundaryField
{
    fuel
    {
        type            calculated;
    }
    air
    {
        type            calculated;
    }
    outlet
    {
        type            calculated;
    }
    frontAndBack
    {
        type            empty;
    }
}


