FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phases (air water);

air
{
    diameterModel   isothermal;
    isothermalCoeffs
    {
        d0              3e-3;
        p0              1e5;
    }
}

water
{
    diameterModel constant;
    constantCoeffs
    {
        d               1e-4;
    }
}

// Virtual-mass ceofficient
Cvm             0.5;

// Lift coefficient
Cl              0;

// Dispersed-phase turbulence coefficient
Ct              1;

// Minimum allowable pressure
pMin            10000;


