phase1
{
    diameterModel constant;
    constantCoeffs
    {
        d               3e-4;
    }
}

phase2
{
    diameterModel constant;
    constantCoeffs
    {
        d               1;
    }
}

// Virtual-mass ceofficient
Cvm             0;

// Lift coefficient
Cl              0;

// Dispersed-phase turbulence coefficient
Ct              0;

